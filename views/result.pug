doctype html
html(lang='en')
  head
    script(src='http://code.jquery.com/jquery-1.11.3.min.js')
    script(src='http://code.jquery.com/ui/1.11.4/jquery-ui.min.js')
    script(src='http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js', type='text/javascript')
    script(type='text/javascript', src='https://cdn.jsdelivr.net/jquery/latest/jquery.min.js')
    script(type='text/javascript', src='http://code.jquery.com/jquery-1.7.1.min.js')
    script(type='text/javascript', src='https://code.jquery.com/jquery-latest.min.js')
    link(href='https://fonts.googleapis.com/css?family=Quicksand', rel='stylesheet')
    link(href='https://fonts.googleapis.com/css?family=Merienda:400,700', rel='stylesheet')
    link(href='https://fonts.googleapis.com/css?family=Merienda', rel='stylesheet')
    link(href='https://fonts.googleapis.com/css?family=Raleway', rel='stylesheet')
    style.
      * {
      margin: 0;
      padding: 0;
      }
      body { padding-top: 70px; }
      .container {
      margin-top:130px;
      width : 60%;
      background-color:white;
      padding-top: 20px;
      padding-left:10px;
      padding-right:20px;
      text-align:center;
      margin-left:21%;
      margin-bottom:100px;
      }
      #bottom_bar {
      width: 100%;
      background-color: black;
      text-align : center;
      color  : white;
      font-family: 'Quicksand', sans-serif;
      font-size : 12px;
      margin-top : 250px;
      padding-top: 10px;
      padding-bottom:10px;
      }
      .navbar {
      width: 100%;
      background-color: black;
      overflow: auto;
      position: fixed; /* Set the navbar to fixed position */
      top: 0; /* Position the navbar at the top of the page */
      z-index:1;
      }
      .navbar a {
      float: left;
      color: white;
      padding-top:25px;
      padding-bottom:25px;
      text-decoration: none;
      font-size: 15px;
      width: 20%; /* Four links of equal widths */
      text-align: center;
      font-family: 'Quicksand', sans-serif;
      }
      .navbar a:hover {
      color: #afacac;
      }
      #header {
      color: black;
      font-family: 'Merienda', cursive;
      text-align: center;
      font-size:45px;
      padding-top:100px;
      margin-bottom:20px;
      }
      #sub_header {
      color: black;
      font-family: 'Merienda', cursive;
      text-align: center;
      font-size:25px;
      padding-top:5px;
      padding-bottom:45px;
      }
      * {box-sizing:border-box}
      /* Slideshow container */
      .slideshow-container {
      max-width: 900px;
      position: relative;
      margin: auto;
      }
      /* Hide the images by default */
      .mySlides {
      display: none;
      }
      /* The dots/bullets/indicators */
      .dot {
      cursor: pointer;
      height: 15px;
      width: 15px;
      margin: 0 2px;
      background-color: #bbb;
      border-radius: 50%;
      display: inline-block;
      transition: background-color 0.6s ease;
      }
      .active, .dot:hover {
      background-color: #717171;
      }
      /* Fading animation */
      .fade {
      opacity:0;
      animation-name: fade;
      animation-duration: 9s;
      animation-iteration-count: infinite;
      left:0;
      right:0;
      }
      @keyframes fade {
      0%   { opacity: 0; }
      11.11%   { opacity: 1; }
      33.33%  { opacity: 1; }
      55.55%  { opacity: 0; }
      100% { opacity: 0; }
      }
      #quiz_box {
      width : 70%;
      margin-top: 180px;
      margin-left: 15%;
      height : 500px;
      position : relative;
      }
      #text_block {
      position : absolute;
      top   : 150px;
      left  : 260px;
      height  : 350px;
      width  : 400px;
      background-color: #161616;
      color: white;
      padding-top: 50px;
      padding-left: 30px;
      padding-right: 30px;
      padding-bottom : 50px;
      text-align: center;
      font-family: 'Raleway', sans-serif;
      }
      .button {
      border: 2px solid white;
      background-color: #161616;
      color: white;
      padding-left: 5px;
      padding-top : 5px;
      padding-bottom : 5px;
      padding-right : 5px;
      font-size: 15px;
      cursor: pointer;
      width : 20%;
      }
      .button:hover {
      background-color: white;
      color: #161616;
      }
  body
    .navbar
      a#home(href='#header') HOME
      a#about(href='#about_section') ABOUT
      a#match(href='#quiz_box') MATCH
      a#browse BROWSE
      a#ticket TICKETS
    div
      p#header  Welcome to G's Musical Land
      p#sub_header  ~ Find Your Soul Musical ~
    .container
      a(name='about_section', style="font-family: 'Quicksand', sans-serif;color:black;font-size:35px;")  ABOUT
      br
      br
      br
      p(style="font-family: 'Quicksand', sans-serif; font-size:20px;line-height:30px;") Here are the filtered titles #{Titles}!
    #bottom_bar
      p  Created 2019 by Georgina Xu. Visit broadway.org for more information.